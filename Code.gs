            const start = new Date(startDate.split('.').reverse().join('-'));
            const end = new Date(endDate.split('.').reverse().join('-'));
            const daysDiff = Math.round((end - start) / (1000 * 60 * 60 * 24)) + 1;

            return `${daysDiff} –¥.`;
        }

